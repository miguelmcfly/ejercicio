        <ul class="navbar-nav">

          <li class="nav-item active">
            <a href="/" class="nav-link">Inicio</a>
          </li>
          <li class="nav-item active">
            <a href="captura" class="nav-link">Captura</a>
          </li>
          <li class="nav-item">
              <a href="listado" class="nav-link">Listado</a>
          </li>
          <li class="nav-item">
            <a href="evaluacion" class="nav-link">Evaluación</a>
          </li>

        </ul>
